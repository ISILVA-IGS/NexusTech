<aside class="main-sidebar">

    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">

      <!-- Sidebar user panel (optional) -->
      <div class="user-panel">
        <div class="pull-left image">
          <img id="imgUser" src="<%=user.foto%>" class="img-circle" alt="User Image">
        </div>
        <div class="pull-left info">
          <p><%=user.Nome_completo%></p>
          <p style="font-size: 13px">CRF: <%=user.CRF%></p>


          <!-- Status -->
        </div>
      </div>

      <!-- Sidebar Menu -->
      <ul class="sidebar-menu tree" data-widget="tree">
        <li class="header">MENU</li>
        <!-- Optionally, you can add icons to the links -->
        
        <li id="sensores" class="active treeview menu-open">
          <a href="#">
              <ion-icon name="analytics"></ion-icon>
            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512" style="width: 20px;height: 20px;"><path d="M405.333 80h-87.35C310.879 52.396 285.821 32 256 32s-54.879 20.396-61.983 48h-87.35C83.198 80 64 99.198 64 122.667v314.665C64 460.801 83.198 480 106.667 480h298.666C428.802 480 448 460.801 448 437.332V122.667C448 99.198 428.802 80 405.333 80zM256 80c11.729 0 21.333 9.599 21.333 21.333s-9.604 21.334-21.333 21.334-21.333-9.6-21.333-21.334S244.271 80 256 80zm152 360H104V120h40v72h224v-72h40v320z"/></svg>
            <span>Sensores</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <%if(sensores.length > 0) { %>

          <ul class="treeview-menu">
            <% sensores.forEach((sensor)=>{ %>
            <li class="active"><a href="/admin/monitoramento?id=<%=sensor.idSensor%>"><i class="fa fa-circle-o"></i><%=sensor.apelido%></a></li>

            <% }) %>
          </ul>


          <% } %>

        </li>
        
        <li>

         
          <a href="/admin/user">
            <i class="fa fa-circle"></i>
            <span>Usuarios</span>
          </a>
        </li>
      </ul>
      <!-- /.sidebar-menu -->
    </section>
    <!-- /.sidebar -->
  </aside>